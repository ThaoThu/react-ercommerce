  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "npm run prettier --write",
      "git add"
    ]
  },
  "name": "campus-ercommerce-frontend",
  "scripts": {
    "build": "REACT_APP_ENV=production npm-run-all build-css build-js",
    "build-js": "GENERATE_SOURCEMAP=false react-scripts build",
    "eject": "react-scripts eject",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "format": "npm run prettier --write 'src/**/*.{js,jsx,json,css}'",
    "generate": "plop --plopfile generators/index.js",
    "lint": "eslint **/*.js --quiet",
    "lint-fix": "eslint --fix",
    "precommit": "lint-staged",
    "preinstall": "npx npm-force-resolutions",
    "prettier": "prettier --single-quote --print-width=120 --trailing-comma es5 --no-semi",
    "start": "npm-run-all -p  start-js  ",
    "start-js": "react-scripts start SET PORT=4000",
    "test": "react-scripts test --env=jsdom"
  },
  "version": "1.1.0",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
